		{
			"name": "1. Search - Valid Keyword",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200 OK\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Response body has 'data' property and it is an array\", () => {\r",
							"    const jsonData = pm.response.json();\r",
							"    pm.expect(jsonData).to.have.property('data');\r",
							"    pm.expect(jsonData.data).to.be.an('array');\r",
							"});\r",
							"pm.test(\"Response data should not be empty\", () => {\r",
							"    pm.expect(pm.response.json().data).to.not.be.empty;\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"packages": {},
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "http://localhost:8091/products/search?q=Hammer",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "8091",
					"path": [
						"products",
						"search"
					],
					"query": [
						{
							"key": "q",
							"value": "Hammer"
						}
					]
				}
			},
			"response": []
		},
